    <record model="ir.sequence" id="l10n_cr_einvoice_sequence_factura_electronica">
            <field name="name">Secuencia Factura Electronica</field>
            <field name="active">TRUE</field>
            <field name="prefix">0010000101</field>
            <field name="padding">10</field>
            <field name="number_increment">1</field>
            <field name="number_next_actual">300</field>
            <field name="implementation">no_gap</field>
             <field name="company_id" ref = "base.main_company"/>
    </record>
        
